{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack --hostname 0.0.0.0",
    "build": "next build",
    "build:production": "NODE_ENV=production next build",
    "start": "next start",
    "start:production": "NODE_ENV=production next start",
    "postinstall": "prisma generate",
    "postbuild": "next-sitemap",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:migrate": "npx prisma migrate deploy",
    "db:seed:css": "npx tsx prisma/seedCssTutorials.ts",
    "db:seed:html": "npx tsx prisma/seedHtmlTutorials.ts",
    "db:seed:dsa": "npx tsx prisma/seedDsaTutorials.ts",
    "db:seed:all": "npx tsx prisma/seedAll.ts",
    "db:reset": "npx prisma migrate reset --force",
    "db:studio": "npx prisma studio",
    "docker:build": "docker build -t vibed-to-cracked .",
    "docker:run": "docker run -p 3000:3000 vibed-to-cracked",
    "deploy:setup": "cp .env.production.example .env.production",
    "deploy:up": "docker-compose up -d --build",
    "deploy:down": "docker-compose down",
    "deploy:logs": "docker-compose logs -f",
    "deploy:backup": "docker-compose exec db pg_dump -U postgres vibed_to_cracked > backup_$(date +%Y%m%d_%H%M%S).sql",
    "data:validate": "node scripts/validate-data.js",
    "data:migrate": "node scripts/migrate-data.js",
    "data:fix": "npm run data:migrate && npm run data:validate"
  },
  "dependencies": {
    "@codemirror/basic-setup": "^0.20.0",
    "@codemirror/lang-javascript": "^6.2.4",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@monaco-editor/react": "^4.7.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/mdx": "^15.4.1",
    "@octokit/rest": "^22.0.0",
    "@prisma/client": "^6.17.1",
    "@stripe/stripe-js": "^7.5.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@types/canvas-confetti": "^1.9.0",
    "@types/mdx": "^2.0.13",
    "@types/nodemailer": "^6.4.17",
    "@webcontainer/api": "^1.6.1",
    "canvas-confetti": "^1.9.3",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.0",
    "framer-motion": "^12.23.6",
    "gray-matter": "^4.0.3",
    "jszip": "^3.10.1",
    "lucide-react": "^0.525.0",
    "mailchecker": "^6.0.18",
    "next": "15.4.10",
    "next-auth": "^4.24.11",
    "next-mdx-remote": "^5.0.0",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.6",
    "nodemailer": "^6.10.1",
    "prisma": "^6.17.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-email": "^4.2.8",
    "react-syntax-highlighter": "^15.6.1",
    "recharts": "^3.2.1",
    "stripe": "^18.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.1",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
